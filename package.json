{
  "name": "ts-react-project",
  "version": "1.0.0",
  "description": "A pipeline for building reactjs-based applications in typescript",
  "private": true,
  "scripts": {
    "prepare-env": "ts-node ./scripts/prepare-env",
    "start": "concurrently \"tsc -w -p ./config/dev.tsconfig.json\" \"http-server build -p 8080 -o\"",
    "build": "webpack --config ./config/webpack.config.js",
    "dist": "http-server dist -p 8081 -o",
    "test": "karma start config/karma.conf.js --browsers ChromeHeadless --single-run",
    "test-debug": "karma start config/karma.conf.js",
    "coverage": "http-server -p 8082 coverage -o"
  },
  "author": "Andrew Lowndes",
  "license": "MIT",
  "devDependencies": {
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.3.6",
    "@types/react": "^16.4.2",
    "@types/react-dom": "^16.0.6",
    "concurrently": "^3.6.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "http-server": "^0.11.1",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-typescript": "^3.0.12",
    "systemjs": "^0.21.4",
    "ts-loader": "^4.4.1",
    "ts-node": "^6.2.0",
    "typescript": "^2.9.2",
    "webpack": "^4.12.1",
    "webpack-cli": "^2.1.5"
  },
  "dependencies": {
    "aphrodite": "^2.2.2",
    "es6-shim": "^0.35.3",
    "normalize.css": "^8.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
}
